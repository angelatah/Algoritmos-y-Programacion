// Algoritmo que evalua una expresion matemática (A+7*C)/(B+2-A)+2*B
Algoritmo SEC10
	
	Definir A,B,C Como real;
	Escribir "Introduzca A: ";
	Leer A;
	Escribir "Introduzca B: ";
	Leer B;
	Escribir "Introduzca c: ";
	Leer C;
	
	Escribir "El resultado de la expresion matemática (A+7*C)/(B+2-A)+2*B es: ", (A+7*C)/(B+2*A)+2*B;
	
FinAlgoritmo
