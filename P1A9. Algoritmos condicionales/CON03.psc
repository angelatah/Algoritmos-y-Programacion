Algoritmo CON03
	definir numero como entero;
	
	escribir "introduzca un número entero: ";
	leer numero;
	
	si numero % 5 = 0 entonces 
		escribir "el numero introducido es multiplo de 5";
	SiNo
		escribir "el numero introducido no es multiplo de 5";
	FinSi
FinAlgoritmo
